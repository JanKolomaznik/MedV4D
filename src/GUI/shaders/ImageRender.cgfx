uint2
GetBBoxEdgePoints( uint idx )
{
	if( idx < 4 ) {
		return uint2( idx, (idx + 1) % 4 );
	}

	if( idx < 8 ) {
		return uint2( idx % 4, idx );
	}
	if( idx < 11 ) {
		return uint2( idx - 4, idx - 3 );
	}
	return uint2( idx - 4, 4 );
}


