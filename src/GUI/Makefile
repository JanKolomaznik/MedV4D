
MAKE=make


.PHONY:all
all:	widgets

.PHONY:clean-all
clean-all:	clean-widgets
#------------------------------------------------------------------------------
.PHONY:widgets
widgets:
	 $(MAKE) --directory=$@
#------------------------------------------------------------------------------

#------------------------------------------------------------------------------
.PHONY:clean-widgets
clean-widgets:
	 $(MAKE) --directory=widgets clean
#------------------------------------------------------------------------------
