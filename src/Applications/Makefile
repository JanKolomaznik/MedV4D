
MAKE=make


.PHONY:all
all:	m4dPilot

.PHONY:clean-all
clean-all:	clean-m4dPilot
#------------------------------------------------------------------------------
.PHONY:m4dPilot
m4dPilot:
	 $(MAKE) --directory=$@
#------------------------------------------------------------------------------

#------------------------------------------------------------------------------
.PHONY:clean-m4dPilot
clean-m4dPilot:
	 $(MAKE) --directory=m4dPilot clean
#------------------------------------------------------------------------------
